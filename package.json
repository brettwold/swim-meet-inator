{
  "name": "SwimResultinator",
  "version": "1.0.0",
  "description": "SwimResultinator application",
  "author": "Brett Cherrington",
  "license": "Apache-2.0",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "./node_modules/.bin/babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "mocha --compilers js:babel-core/register",
    "test-bp": "mocha --require should test/bp-host-*.js --compilers js:babel-core/register",
    "lint": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "dependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "body-parser": "^1.15.2",
    "cheerio": "^0.20.0",
    "connect-ensure-login": "^0.1.1",
    "cookie-parser": "~1.3.5",
    "csv-parse": "^1.1.7",
    "debug": "~2.2.0",
    "dotenv": "^2.0.0",
    "eslint": "^3.1.1",
    "express": "^4.14.0",
    "express-session": "^1.14.2",
    "json": "^9.0.4",
    "mkdirp": "^0.5.1",
    "mocha": "^3.2.0",
    "moment": "^2.17.1",
    "morgan": "~1.6.1",
    "multer": "^1.2.0",
    "nodemon": "^1.9.2",
    "passport": "^0.3.2",
    "passport-bitbucket-oauth2": "^0.1.2",
    "passport-google-oauth20": "^1.0.0",
    "promise": "^7.1.1",
    "request": "^2.79.0",
    "sequelize": "^3.30.0",
    "sequelize-json": "^2.1.2",
    "serve-favicon": "~2.3.0",
    "slugify": "^1.0.2",
    "sqlite3": "^3.1.8",
    "uuid": "^3.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluefins/SwimResultinator.git"
  },
  "keywords": [
    "Swimming",
    "Meets"
  ],
  "bugs": {
    "url": "https://github.com/bluefins/SwimResultinator.git/issues"
  },
  "homepage": "https://github.com/bluefins/SwimResultinator.git#readme"
}
